extends layout

block additionalLibs
  // bower:js
  script(src='../bower_components/es5-shim/es5-shim.js')
  script(src='../bower_components/angular/angular.js')
  script(src='../bower_components/angular-route/angular-route.js')
  script(src='../bower_components/angular-file-upload/angular-file-upload.js')
  script(src='../public/javascripts/picsglobal.js')
  // endbower

block content
  div(ng-app='picsGlobal' ng-controller='mainController')
    h1= title
    p Welcome to #{title}
    p This page is still work in progress, check later to see how it grows!
    p(class='smallText') {{test}}
